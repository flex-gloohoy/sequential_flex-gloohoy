numbers = []
numbers_2 = []
aft_numbers = []

c = input("how much numbers? ")
i = 1
while i <= int(c):
  numbers.append(int(input("input one number: ")))
  i+=1

for e in numbers:
    d = (e * 20)
    numbers_2.append(d)

for e in numbers:
    if e == 0:
        aft_numbers.append(1)
    else:
        aft_numbers.append(0)


print (aft_numbers)

